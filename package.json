{
  "name": "commix",
  "npmName": "commix",
  "version": "0.1.0",
  "author": "Billgo",
  "license": "MIT",
  "description": "less color commix",
  "homepage": "https://commix.yiqilai.tech",
  "main": "lib/commix.js",
  "style": "lib/commix.css",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "index.less"
  ],
  "scripts": {
    "prepare": "npm run test && gulp default && npm run build",
    "build": "rimraf lib && mkdir lib && BABEL_ENV=production rollup -c",
    "lint": "prettier-eslint --write src/**/*.{js,jsx}",
    "test": "BABEL_ENV=test jest --no-cache && jest-coverage-badges --output ./coverage"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-common": "^0.2.2",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-connect": "^5.7.0",
    "gulp-ejs": "^4.1.1",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-coverage-badges": "^1.1.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-autoprefix": "^2.0.0",
    "less-plugin-functions": "^1.0.0",
    "less-plugin-glob": "^3.0.0",
    "less-plugin-inline-urls": "^1.2.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.1",
    "rollup-plugin-auto-named-exports": "^1.0.0-beta.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy-glob": "^0.3.0",
    "rollup-plugin-eslint": "^6.0.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-lessimporter": "^0.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-url": "^2.2.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0"
  },
  "babel": {
    "presets": [
      "common"
    ],
    "comments": false,
    "minified": true
  },
  "eslintConfig": {
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "babel",
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "prettier/flowtype",
      "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    "lib/*",
    "docs/*",
    "dist/*",
    "build/*",
    "coverage/*",
    "fixtures/*",
    "umd/*"
  ],
  "stylelint": {
    "extends": "stylelint-config-prettier",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "extends"
          ]
        }
      ],
      "block-no-empty": null,
      "unit-whitelist": [
        "%",
        "px",
        "em",
        "rem",
        "s"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/billgo/commix.git"
  },
  "bugs": "https://github.com/billgo/commix/issues"
}
